<div class="row">
  <%= render 'shared/menu' %>

  <div class="small-12 large-9 columns">
    <div class="radius panel">
      <h1 style="text-align:center;">Delicious</h1>

      <% @feed.entries.each do |bookmark| %>
        <span class="secondary radius label"><%= bookmark.published.strftime('%B %e, %Y') %></span>
        <h4><a href="<%= bookmark.url %>" target="_blank"><%= bookmark.title.html_safe %></a></h4>
        <p style="text-align:right;">
          <% bookmark.categories.each do |category| %>
            <a href="http://www.delicious.com/<%= DELICIOUS_USER_NAME %>/<%= category %>" target="_blank">
              <span class="radius label"><%= category %></span>
            </a>
          <% end %>
        </p>
        <hr>
      <% end %>

      <h6>
        <a href="http://www.delicious.com/<%= DELICIOUS_USER_NAME %>" target="_blank"><%= YOUR_NAME %> @ Delicious</a>
      </h6>
    </div>
  </div>
</div>
